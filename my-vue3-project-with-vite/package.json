{
  "name": "my-vue3-project-with-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode mock",
    "start": "npm run dev",
    "serve-with-jsonserver": "vite --mode jsonserver",
    "serve-local": "vite --mode localapis",
    "build": "vue-tsc --noEmit && vite build --mode productio",
    "build-beta": "vue-tsc --noEmit && vite build --mode beta",
    "build-mock": "vue-tsc --noEmit && vite build --mode mock",
    "preview": "vite preview",
    "serve-json-server-api": "json-server --port 3111 --watch json-server/db.json",
    "pretty": "prettier -w \"./src/**/*.*{ts,vue,css,scss,html}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged \"./src/**/*.*{ts,vue,css,scss,html}\""
    }
  },
  "dependencies": {
    "axios": "^0.26.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.4",
    "husky": "^7.0.4",
    "i18next": "^21.6.14",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.5.4",
    "vite": "^2.8.6",
    "vitest": "^0.6.0",
    "vue-i18n": "^9.1.9",
    "vue-tsc": "^0.29.8"
  }
}
